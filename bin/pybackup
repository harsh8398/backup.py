#!/usr.bin/python3

import pybackup
from sys import argv

script, src, dst = argv
obj=pybackup.GetList(src, dst)
mylist=obj.flist()
obj1=pybackup.CreateDirs(obj.src,obj.dst)
success=obj1.makeDirs(mylist)
if success:
    obj2=pybackup.BckpList(obj1.src,obj1.dst)
    print(obj2.docopy(mylist))
else:
    print("Script returned with error")
